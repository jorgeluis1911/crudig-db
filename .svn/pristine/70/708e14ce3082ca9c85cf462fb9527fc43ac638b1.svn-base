
.form-control {
	padding: 3px 3px;
	height: 27px;
	font-size: 15px;
	line-height: 1.0;	
}

.container {
	margin-left: 0px;
	margin-right: 0px;
	width: auto;
}

.navbar {
	border-radius: 0px;
}

table {
	font-size: 12px;
}

.pagination {
	margin: 0px;
}

.panel-heading, .panel-footer {
	padding: 6px 6px;
}

.table-condensed > tbody > tr > td,
.table-condensed > thead > tr > th {
	padding: 3px;
}

.pagination > li > a, .pagination > li > span {
	padding: 2px 10px;
}	
