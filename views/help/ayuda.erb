            <h4 id="configuraciones_href">Configuraciones</h4>
            <p>texto</p>
            
            <h4 id="usuarios_href">Usuarios</h4>
            <p>texto</p>
          </div>
          
        </div>
        
        <div class="col-md-2 menu_ayuda">
          <ul style="position: relative;">

            <li class="">
              <a href="#modals">Listados</a>
              <ul>
                <li class=""><a href="#insercion_href">Inserción</a></li>
                <li class=""><a href="#edicion_href">Edición</a></li>
                <li class=""><a href="#busquedas_href">Busquedas</a></li>
                <li class=""><a href="#autorefresco_href">Autorefresco</a></li>
