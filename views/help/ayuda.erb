              <ul>
                <li><a href="#area_href">Área</a></li>
                <li><a href="#circulo_href">Círculos</a></li>
                <li><a href="#barras_href">Barras</a></li>
                <li><a href="#columnas_href">Columnas</a></li>
                <li><a href="#combo_href">Combo</a></li>
                <li><a href="#lineas_href">Líneas</a></li>
                <li><a href="#misgraficos_href">Mis gráficos</a></li>
              </ul>
            </li>
            <li class="">
              <a href="#config_href">Config</a>
              <ul>
                <li><a href="#configuraciones_href">Configuraciones</a></li>
                <li><a href="#usuarios_href">Usuarios</a></li>
              </ul>
            </li>

          </ul>
        </div>
      </div>
    </div>
          
      
  </body>
</html>       